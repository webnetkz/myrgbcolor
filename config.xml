<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "myRGBcolor"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>myRGBcolor</name>

  <description>
      myRGBcolor application
  </description>

  <author href="https://webnet.kz" email="info@webnet.kz">
      WebNet
  </author>
    
        <!-- Define app icon and splashscreen for each platform. -->
    <platform name="android">
        <icon   src="logo.png"               density="ldpi" />
        <icon   src="logo.png"               density="mdpi" />
        <icon   src="logo.png"               density="hdpi" />
        <icon   src="logo.png"              density="xhdpi" />
        <icon   src="logo.png"             density="xxhdpi" />
        <icon   src="logo.png"            density="xxxhdpi" />
        <splash src="logo.png"      density="land-ldpi" />
        <splash src="logo.png"      density="land-mdpi" />
        <splash src="logo.png"      density="land-hdpi" />
        <splash src="logo.png"     density="land-xhdpi" />
        <splash src="logo.png"    density="land-xxhdpi" />
        <splash src="logo.png"   density="land-xxxhdpi" />
        <splash src="logo.png"      density="port-ldpi" />
        <splash src="logo.png"      density="port-mdpi" />
        <splash src="logo.png"      density="port-hdpi" />
        <splash src="logo.png"     density="port-xhdpi" />
        <splash src="logo.png"    density="port-xxhdpi" />
        <splash src="logo.png"   density="port-xxxhdpi" />
    </platform>

</widget>
